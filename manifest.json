{
  "manifest_version": 3,

  "name": "ジャバ人間ブラウザ",
  "short_name" : "ジャバ人間ブラウザ",
  "description": "あなたのブラウザにジャバが来ます。",
  "version": "10.1",

  "action": {
    "default_icon": {
      "16": "icon16.png",
      "24": "icon24.png",
      "32": "icon32.png",
      "128": "icon128.png"
    },
    "default_popup": "popup.html",
    "default_title": "ジャバ人間ブラウザ"
  },
   
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },

  "host_permissions": [
    "https://arigato-java.download/"
  ]
}
